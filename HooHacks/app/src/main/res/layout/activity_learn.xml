<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".Learn">

    <androidx.constraintlayout.widget.ConstraintLayout

        android:id="@+id/layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
            android:id="@+id/youTubePlayerView2"
            android:layout_width="321dp"
            android:layout_height="147dp"
            app:autoPlay="false"
            app:layout_constraintBottom_toTopOf="@+id/gotoTrans"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.266"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.175"
            app:showYouTubeButton="false"
            app:videoId="v1desDduz5M" />

        <com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
            android:id="@+id/youTubePlayerView4"
            android:layout_width="321dp"
            android:layout_height="161dp"
            app:autoPlay="false"
            app:layout_constraintBottom_toTopOf="@+id/gotoTrans"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.24"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/youTubePlayerView2"
            app:layout_constraintVertical_bias="0.029"
            app:showYouTubeButton="false"
            app:videoId="a5BD8SjhPSg" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="364dp"
            android:layout_height="24dp"
            android:text="@string/linktosite"
            app:layout_constraintBottom_toTopOf="@+id/gotoTrans"
            app:layout_constraintEnd_toEndOf="@+id/youTubePlayerView4"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/youTubePlayerView4"
            app:layout_constraintTop_toBottomOf="@+id/youTubePlayerView4"
            app:layout_constraintVertical_bias="0.292" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="383dp"
            android:layout_height="18dp"
            android:text="@string/udemy"
            app:layout_constraintBottom_toTopOf="@+id/gotoTrans"
            app:layout_constraintEnd_toEndOf="@+id/textView"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintVertical_bias="0.10" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="377dp"
            android:layout_height="20dp"
            android:text="@string/lynda"
            app:layout_constraintBottom_toTopOf="@+id/gotoTrans"
            app:layout_constraintEnd_toEndOf="@+id/textView1"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView1"
            app:layout_constraintTop_toBottomOf="@+id/textView1"
            app:layout_constraintVertical_bias="0.10" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="371dp"
            android:layout_height="22dp"
            android:text="@string/edx"
            app:layout_constraintBottom_toTopOf="@+id/gotoTrans"
            app:layout_constraintEnd_toEndOf="@+id/textView2"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView2"
            app:layout_constraintTop_toBottomOf="@+id/textView2"
            app:layout_constraintVertical_bias="0.10" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="369dp"
            android:layout_height="19dp"
            android:text="@string/skillshare"
            app:layout_constraintBottom_toTopOf="@+id/gotoTrans"
            app:layout_constraintEnd_toEndOf="@+id/textView3"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView3"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            app:layout_constraintVertical_bias="0.10" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="418dp"
            android:layout_height="77dp"
            android:background="#009688"
            android:textColorLink="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <Button
            android:id="@+id/gotoLearn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="LEARN"
            android:background="#07D6FA"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.05"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.976" />

        <Button
            android:id="@+id/gotoMAP"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Maps"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.95"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.976" />

        <Button
            android:id="@+id/gotoTrans"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Translation"
            android:textSize="10sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.976" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="144dp"
            android:layout_height="50dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="28dp"
            android:fontFamily="sans-serif-black"
            android:text="Videos"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textSize="36sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="144dp"
            android:layout_height="50dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="19dp"
            android:fontFamily="sans-serif-black"
            android:text="Articles"
            android:textAppearance="@style/TextAppearance.AppCompat.Large"
            android:textSize="36sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/youTubePlayerView4" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>